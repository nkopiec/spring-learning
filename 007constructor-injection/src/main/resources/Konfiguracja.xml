<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="mojLogger" class="pl.java.LogerImpl">
	<property name="name" value="mojLogger"/>
	<property name="version" value="3"/>
	</bean>



	<bean id="repozytoriumUzytkownikow" 
		class="pl.java.UsersRepositoryImpl">
		<constructor-arg  ref="mojLogger"></constructor-arg>
		<constructor-arg index="2" value="db"></constructor-arg>
		<constructor-arg index="1" value="http//123422.pl"></constructor-arg>
		</bean>
		
	<bean id="sameBean" 
	class="pl.java.SameBean">
	<constructor-arg value="50"></constructor-arg>
	<constructor-arg value="100"></constructor-arg>
	</bean>
	
</beans>
